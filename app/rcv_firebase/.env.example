# Google Maps API Key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Firebase Web Configuration
FIREBASE_WEB_API_KEY=your_web_api_key
FIREBASE_WEB_APP_ID=your_web_app_id
FIREBASE_WEB_MESSAGING_SENDER_ID=your_messaging_sender_id
FIREBASE_WEB_PROJECT_ID=your_project_id
FIREBASE_WEB_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_WEB_STORAGE_BUCKET=your_project_id.appspot.com
FIREBASE_WEB_MEASUREMENT_ID=your_measurement_id

# Firebase Android Configuration
FIREBASE_ANDROID_API_KEY=your_android_api_key
FIREBASE_ANDROID_APP_ID=your_android_app_id
FIREBASE_ANDROID_MESSAGING_SENDER_ID=your_messaging_sender_id
FIREBASE_ANDROID_PROJECT_ID=your_project_id
FIREBASE_ANDROID_STORAGE_BUCKET=your_project_id.appspot.com

# Firebase iOS Configuration
FIREBASE_IOS_API_KEY=your_ios_api_key
FIREBASE_IOS_APP_ID=your_ios_app_id
FIREBASE_IOS_MESSAGING_SENDER_ID=your_messaging_sender_id
FIREBASE_IOS_PROJECT_ID=your_project_id
FIREBASE_IOS_STORAGE_BUCKET=your_project_id.appspot.com
FIREBASE_IOS_BUNDLE_ID=com.yourcompany.yourapp

# Firebase Windows Configuration
FIREBASE_WINDOWS_API_KEY=your_windows_api_key
FIREBASE_WINDOWS_APP_ID=your_windows_app_id
FIREBASE_WINDOWS_MESSAGING_SENDER_ID=your_messaging_sender_id
FIREBASE_WINDOWS_PROJECT_ID=your_project_id
FIREBASE_WINDOWS_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_WINDOWS_STORAGE_BUCKET=your_project_id.appspot.com
FIREBASE_WINDOWS_MEASUREMENT_ID=your_measurement_id

# IMPORTANT NOTES:
# 1. Copy this file to .env and replace all values with your actual Firebase configuration
# 2. Get your Firebase configuration from Firebase Console > Project Settings > Your Apps
# 3. Firebase API keys are NOT secret - they identify your project, not grant access
# 4. Security is enforced through Firebase Security Rules, not by hiding these keys
# 5. Never commit the actual .env file to version control
